{
  "name": "todo-redux-immutable",
  "description": "Test project for Todo List, with Redux and Immutable.js.",
  "version": "1.0.0",
  "author": "Christopher Barrett",
  "private": false,
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development node webpack/dev-server.js",
    "clean": "rm build/bundle.*",
    "build": "NODE_ENV=production webpack --config webpack/config.prod.js",
    "prod": "npm run build && open build/index.html",
    "dev": "open http://localhost:8080 && npm start",
    "lint": "./node_modules/.bin/eslint webpack src || true",
    "test": "npm run test:actions; npm run test:reducers",
    "test:actions": "./node_modules/.bin/mocha --reporter spec .specsetup.js src/**/*Actions.spec.js",
    "test:reducers": "./node_modules/.bin/mocha --reporter spec .specsetup.js src/**/*Reducer.spec.js"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.2.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.3",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "express": "^4.14.0",
    "mocha": "^2.5.3",
    "postcss-assets": "^4.1.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-url": "^5.1.2",
    "precss": "^1.4.0",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "style-loader": "^0.13.1",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.0"
  },
  "dependencies": {
    "async-retry": "^1.1.4",
    "immutable": "^3.8.2",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "normalize.css": "^4.2.0",
    "react": "^15.6.2",
    "react-addons-pure-render-mixin": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^4.4.8",
    "redux": "^3.7.2",
    "redux-immutable": "^3.1.0",
    "redux-saga": "^0.16.0",
    "whatwg-fetch": "^2.0.3"
  }
}
